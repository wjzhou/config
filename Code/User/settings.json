{
    "editor.formatOnSave": true,
    "editor.codeActionsOnSave": {
        "source.fixAll": true
    },
    "files.eol": "\n",
    "files.exclude": {
        "**/node_modules": true,
    },
    "typescript.enablePromptUseWorkspaceTsdk": true,
    "vim.easymotion": true,
    "vim.incsearch": true,
    "vim.useSystemClipboard": true,
    "vim.useCtrlKeys": true,
    "vim.hlsearch": true,
    "vim.insertModeKeyBindings": [
        {
            "before": [
                "j",
                "j"
            ],
            "after": [
                "<Esc>"
            ]
        }
    ],
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [
                "g",
                "h"
            ],
            "commands": [
                "editor.action.showDefinitionPreviewHover"
            ]
        },
        {
            "before": [
                "<C-n>"
            ],
            "commands": [
                ":nohl"
            ]
        },
        {
            "before": [
                "<leader>",
                "m"
            ],
            "commands": [
                "bookmarks.toggle"
            ]
        },
        {
            "before": [
                "<leader>",
                "b"
            ],
            "commands": [
                "bookmarks.list"
            ]
        },
        {
            "before": [
                "Z",
                "Z"
            ],
            "commands": [
                ":wq"
            ]
        },
        {
            "before": [
                "<leader>",
                "w"
            ],
            "commands": [
                "workbench.action.files.save",
            ]
        }
    ],
    "vim.statusBarColorControl": true,
    "vim.statusBarColors": {
        "normal": "#005f5f",
        "insert": "#5f0000",
        "visual": "#5f00af",
        "visualline": "#005f87",
        "visualblock": "#86592d",
        "replace": "#000000"
    },
    "editor.lineNumbers": "relative",
    "vim.leader": "<space>",
    "vim.handleKeys": {
        "<C-a>": false,
        "<C-f>": false,
        "<C-v>": false,
    },
    "typescript.updateImportsOnFileMove.enabled": "always",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "vim.changeWordIncludesWhitespace": true,
    "vim.highlightedyank.enable": true,
    "vim.replaceWithRegister": true,
    "window.zoomLevel": 0,
    "editor.rulers": [
        120
    ],
    "python.linting.pylintArgs": [
        "--extension-pkg-whitelist=numpy,cv2"
    ],
    "window.zoomLevel": 1,
    "editor.tabSize": 4,
    "editor.insertSpaces": true,
    "editor.autoIndent": true,
    "editor.detectIndentation": true,
    "C_Cpp.intelliSenseEngine": "Default",
    "files.exclude": {
        "**/*.pyc": true
    },
    "C_Cpp.clang_format_fallbackStyle": "Google",
    "files.associations": {
        "*.bash-completion": "shellscript"
    },
    "terminal.integrated.allowChords": false,
    "diffEditor.ignoreTrimWhitespace": false,
    "cSpell.spellCheckDelayMs": 500,
    "workbench.colorCustomizations": {
        "statusBar.background": "#5f0000",
        "statusBar.noFolderBackground": "#5f0000",
        "statusBar.debuggingBackground": "#5f0000"
    },
    "material-ui-snippets.showNotesOnStartup": false,
}