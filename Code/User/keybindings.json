// Place your key bindings in this file to overwrite the defaults
[
    {
        "key": "ctrl+e",
        "command": "-workbench.action.quickOpen"
    },
    {
        "key": "ctrl+shift+q",
        "command": "workbench.action.toggleMaximizedPanel"
    },
    {
        "key": "ctrl+shift+j",
        "command": "workbench.action.terminal.focusNext"
    },
    {
        "key": "ctrl+shift+k",
        "command": "workbench.action.terminal.focusPrevious"
    },
    {
        "key": "ctrl+o",
        "command": "workbench.action.navigateBack"
    },
    {
        "key": "ctrl+shift+c",
        "command": "editor.action.clipboardCopyAction",
        "when": "editorTextFocus"
    },
    {
        "key": "ctrl+shift+v",
        "command": "editor.action.clipboardPasteAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "ctrl+shift+v",
        "command": "filesExplorer.paste",
        "when": "explorerViewletVisible && fileCopied && filesExplorerFocus && !inputFocus"
    },
    {
        "key": "ctrl+shift+c",
        "command": "filesExplorer.copy",
        "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceIsRoot && !inputFocus"
    },
    {
        "key": "ctrl+shift+c",
        "command": "problems.action.copy",
        "when": "problemFocus"
    },
    {
        "key": "ctrl+shift+c",
        "command": "keybindings.editor.copyKeybindingEntry",
        "when": "inKeybindings && keybindingFocus"
    },
    {
        "key": "ctrl+shift+x",
        "command": "editor.action.clipboardCutAction",
        "when": "editorTextFocus && !editorReadonly"
    },
    {
        "key": "alt+2",
        "command": "workbench.action.terminal.focusAtIndex2"
    },
    {
        "key": "alt+1",
        "command": "workbench.action.terminal.focusAtIndex1"
    },
    {
        "key": "alt+3",
        "command": "workbench.action.terminal.focusAtIndex3"
    },
    {
        "key": "alt+4",
        "command": "workbench.action.terminal.focusAtIndex4"
    },
    {
        "key": "alt+5",
        "command": "workbench.action.terminal.focusAtIndex5"
    },
    {
        "key": "alt+6",
        "command": "workbench.action.terminal.focusAtIndex6"
    },
    {
        "key": "alt+7",
        "command": "workbench.action.terminal.focusAtIndex7"
    },
    {
        "key": "alt+8",
        "command": "workbench.action.terminal.focusAtIndex8"
    },
    {
        "key": "alt+9",
        "command": "workbench.action.terminal.focusAtIndex9"
    },
]